              {
                "label": "Aprobar",
                "action": {
                  "fnKey": "showConfirm",
                  "params": {
                    "message": "Aprobar comentario?",
                    "onAccept": {
                      "fnKey": "multipleActions",
                      "params": {
                        "actions": [
                          {
                            "action": {
                              "fnKey": "uiShowLoader"
                            }
                          },
                          {
                            "action": {
                              "fnKey": "callService",
                              "params": {
                                "serviceName": "updateCommentStatus",
                                "serviceParams": {
                                  "|&|commentId": "&.item._id",
                                  "status": "APPROVED"
                                },
                                "onSuccess": {
                                  "fnKey": "reloadBlock"
                                },
                                "onError": {
                                  "fnKey": "showAlert",
                                  "params": {
                                    "message": "Error aprobando comentario"
