          "asd": 12345
        }
      }
    }
  ],
  "states": [
    {
      "key": "beach-price",
      "params": {
        "serviceDataStores": [],
        "blockType": "data-table",
        "blockParams": {
          "title": "Todos Lista",
          "tableServiceDataStores": [
            {
              "key": "todos",
              "serviceName": "todoGet",
              "serviceParams": {}
            }
          ],
          "headerOptions": [
            {
              "type": "action",
              "label": "Registrar Todo",
              "action": {
                "fnKey": "goToState",
                "params": {
                  "stateName": "register-beach-price",
                  "stateParams": {}
                }
              }
            }
          ],
          "listSetup": {
            "|&|source": "&.tableServiceDataStores.todos",
            "options": [
              {
                "label": "Editar",
                "action": {
                  "fnKey": "goToState",
                  "params": {
                    "stateName": "update-todo",
                    "stateParams": {
                      "|&|todoId": "&.item._id"
                    }
                  }
                }
              },
              {
                "label": "Borrar",
                "action": {
                  "fnKey": "callService",
                  "params": {
                    "serviceName": "removeTodo",
                    "serviceParams": {
                      "|&|itemId": "&.item._id"
                    },
                    "onSuccess": {
                      "fnKey": "reloadTable"
                    }
                  }
                }
              }
            ],
            "mapping": [
              {
                "label": "Nombre",
                "content": {
                  "|&|data": "&.item.name"
                }
              }
            ]
          }
        }
      }
    },
    {
      "key": "register-beach-price",
      "params": {
        "serviceDataStores": [],
        "blockType": "form",
        "blockParams": {
          "title": "Registrar todo",
          "headerOptions": [
            {
              "type": "action",
              "label": "Volver a lista de todos",
              "action": {
                "fnKey": "goToState",
                "params": {
                  "stateName": "beach-price",
                  "stateParams": {}
                }
              }
            }
          ],
          "dynamicRender": true,
          "formItems": [
            {
              "type": "input",
              "setup": {
                "label": "Nombre",
                "outputAlias": "name",
                "type": "text"
              }
            },
            {
              "type": "action",
              "setup": {
                "label": "Enviar Datos",
                "action": {
                  "fnKey": "callService",
                  "params": {
                    "serviceName": "todoAdd",
                    "serviceParams": {
                      "|&|name": "&.formOutputsMap.name"
                    },
                    "onSuccess": {
                      "fnKey": "goToState",
                      "params": {
                        "stateName": "beach-price",
                        "stateParams": {}
                      }
                    },
                    "onError": {
                      "fnKey": "showAlert",
                      "params": {
                        "message": "Error registrando"
                      }
                    }
                  }
                }
              }
            }
          ]
        }
      }
    },
    {
      "key": "update-todo",
      "params": {
        "serviceDataStores": [
          {
            "key": "todoData",
            "serviceName": "getTodoById",
            "serviceParams": {
              "|&|todoId": "&.currentStateParams.todoId"
            }
          }
        ],
        "blockType": "form",
        "blockParams": {
          "title": "Editar Todo",
          "headerOptions": [
            {
              "type": "action",
              "label": "Volver a lista de todos",
              "action": {
                "fnKey": "goToState",
                "params": {
                  "stateName": "beach-price",
                  "stateParams": {}
                }
              }
            }
          ],
          "dynamicRender": true,
          "formItems": [
            {
              "type": "input",
              "setup": {
                "label": "Nombre",
                "outputAlias": "name",
                "|&|initValue": "&.serviceDataStores.todoData.name",
                "type": "text"
              }
            },
            {
              "type": "action",
              "setup": {
                "label": "Enviar Datos",
                "action": {
                  "fnKey": "callService",
                  "params": {
                    "serviceName": "todoUpdate",
                    "serviceParams": {
                      "|&|name": "&.formOutputsMap.name",
                      "|&|todoId": "&.currentStateParams.todoId"
                    },
                    "onSuccess": {
                      "fnKey": "goToState",
                      "params": {
                        "stateName": "beach-price",
                        "stateParams": {}
                      }
                    },
                    "onError": {
                      "fnKey": "showAlert",
                      "params": {
                        "message": "Error registrando"
                      }
                    }
                  }
                }
              }
            }
          ]
        }
      }
    }
  ]
